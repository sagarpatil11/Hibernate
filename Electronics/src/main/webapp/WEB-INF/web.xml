<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ProductDto</servlet-name>
  	<display-name>ProductDto</display-name>
  	<description></description>
  	<servlet-class>com.electronics.DTO.ProductDto</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Product</servlet-name>
  	<display-name>Product</display-name>
  	<description></description>
  	<servlet-class>com.electronics.controller.Product</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ShowProduct</servlet-name>
  	<display-name>ShowProduct</display-name>
  	<description></description>
  	<servlet-class>com.electronics.controller.ShowProduct</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ShowProductDetails</servlet-name>
  	<display-name>ShowProductDetails</display-name>
  	<description></description>
  	<servlet-class>com.electronics.controller.ShowProductDetails</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ProductDto</servlet-name>
  	<url-pattern>/ProductDto</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Product</servlet-name>
  	<url-pattern>/Product</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ShowProduct</servlet-name>
  	<url-pattern>/ShowProduct</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ShowProductDetails</servlet-name>
  	<url-pattern>/ShowProductDetails</url-pattern>
  </servlet-mapping>
</web-app>
